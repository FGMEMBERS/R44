<?xml version="1.0"?>

<PropertyList>

    <path>airvent.ac</path>
    <animation>
        <type>translate</type>
        <object-name>AirVent</object-name>
        <property>sim/Aircraft/R44/airvent</property>
        <factor>-0.025</factor>
        <axis>
            <x>1</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>slider</type>
        <object-name>AirVent</object-name>
        <visible>true</visible>
        <drag-direction>vertical</drag-direction>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/Aircraft/R44/airvent</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1</max>
                <wrap>0</wrap>
            </binding>
        </action>
    </animation>
    
</PropertyList>
 
