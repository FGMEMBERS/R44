    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LHpedal</object-name>
    <property>controls/flight/rudder</property>
    <factor> -20 </factor>
    <center>
      <x-m> -0.123 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.361 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Collective</object-name>
    <property>controls/engines/engine/throttle</property>
